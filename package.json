{
  "name": "wolf.js",
  "version": "3.0.0-dev1",
  "description": "The World's Online Festival API",
  "keywords": [
    "wolf",
    "palringo",
    "the world's online festival",
    "wolfringo",
    "pal"
  ],
  "files": [
    "dist/",
    "wrapper.mjs",
    "!**/*.tsbuildinfo"
  ],
  "directories": {
    "lib": "lib/"
  },
  "type": "commonjs",
  "main": "./dist/index.js",
  "exports": {
    "import": "./wrapper.mjs",
    "require": "./dist/index.js"
  },
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kosterc/wolf.js.git"
  },
  "scripts": {
    "compile": "rimraf ./dist && tsc",
    "devCompile": "rimraf ./dist && tsc -w",
    "prepack": "npm run compile"
  },
  "dependencies": {
    "@types/node": ">=10.0.0",
    "@types/socket.io-client": "^1.4.36",
    "@types/winston": "^2.4.4",
    "blueimp-md5": "^2.18.0",
    "socket.io-client": "^2.4.0",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "typescript": "^4.1.2"
  },
  "contributors": [
    {
      "name": "Christopher Koster",
      "email": "christopher@kostertech.io"
    }
  ],
  "engines": {
    "node": ">=10.0.0"
  },
  "bugs": {
    "url": "https://github.com/kosterc/wolf.js/issues"
  },
  "homepage": "https://github.com/kosterc/wolf.js#readme"
}
